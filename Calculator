#include<iostream>
using namespace std;

int main()
{
   int a, b;
   char op;
   cin>>a>>op>>b;
   switch(op)
   {
    case '+':
    {cout<<"Sum = "<<a+b;
    break;
    }

   case '-':
    {cout<<"Difference = "<<a-b;
    break;
    }
   
   case '*':
    {cout<<"Multiplication = "<<a*b;
    break;
    }
   
   case '/':
    {cout<<"Division:"<<endl<<"Quotient= "<<a/b << " Remainder= "<<a%b;
    break;
    }
    default:
    {
        cout<<"Enter a valid input";

    }
   }
   return 0;
}
